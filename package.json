{
  "name": "seeds",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@manypkg/cli": "^0.24.0",
    "@types/node": "^24.0.0",
    "eslint": "^9.28.0",
    "knip": "^5.60.2",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "syncpack": "14.0.0-alpha.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "private": true,
  "repository": "https://github.com/mike-north/ts-monorepos-v2",
  "scripts": {
    "build": "pnpm --color run -r build",
    "check": "pnpm --color run -r check",
    "dev": "pnpm --color run -r dev",
    "format": "prettier --write packages/*/{src,tests}/**/*.ts packages/*/{tailwind.config.js,postcss.config.cjs,vite.config.ts,svelte.config.js}",
    "lint": "pnpm --color run -r lint && pnpm --color manifests-lint",
    "manifests-fix": "pnpm manypkg fix",
    "manifests-lint": "pnpm manypkg check",
    "test": "pnpm --color run -r test",
    "knip": "knip"
  },
  "volta": {
    "node": "22.16.0"
  }
}
